{
  "name": "tgallice/fb-messenger-sdk",
  "description": "Facebook Messenger Bot php sdk",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Gallice",
      "email": "tm.gallice@gmail.com"
    }
  ],
  "keywords": [
    "facebook",
    "messenger",
    "bot messenger",
    "facebook messenger",
    "bot",
    "conversation",
    "bot conversation"
  ],
  "autoload": {
    "psr-4": {
      "Tgallice\\FBMessenger\\": "src"
    }
