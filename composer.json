    "symfony/polyfill-php56": "^1.2",
    "symfony/event-dispatcher": "^3.1"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
